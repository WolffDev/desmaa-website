  "songVers": [
    [
      "Søren henter mælk til mor,",
      "synes, han er vældig stor,",
      "når han går med spanden",
      "ned til mælkemanden."
    ],
    [
      "Mens han venter på sin spand,",
      "står han kæk, den lille mand,",
      "klapper hestens mule.",
      "Bange? Ej en smule!"
    ],
    [
      "Op ad trappen tripper han",
      "med den fyldte mælkespand;",
      "men en dag det hændte,",
      "mælken fra ham rendte."
    ],
    [
      "Mens han trådte fejl et sted,",
      "faldt hans spand ad trappen ned;",
      "mælken flød til døren –",
      "stakkels lille Søren!"
    ],
    [
      "Tænk, den rare mælkemand",
      "fyldte atter Sørens spand,",
      "bad ham så gå rigtig",
      "stille og forsigtig."
    ]
  ],
